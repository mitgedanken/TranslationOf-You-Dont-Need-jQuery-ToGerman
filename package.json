  ],
  "author": "OneUI Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oneuijs/You-Dont-Need-jQuery/issues"
  },
  "homepage": "https://github.com/oneuijs/You-Dont-Need-jQuery",
