    "husky": "^7.0.1",
    "jquery": "^3.6.0",
    "karma": "^0.13",
    "karma-firefox-launcher": "^0.1",
    "karma-mocha": "^0.2.1",
    "lint-staged": "^11.0.1",
    "mocha": "^2.3",
    "prettier": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oneuijs/You-Dont-Need-jQuery.git"
  },
  "keywords": [
    "convertion guide",
